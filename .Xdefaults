URxvt.buffered:         true
URxvt.background:       rgb:00/00/00
URxvt.foreground:       white
URxvt.cursorColor:      green
URxvt.underlineColor:   yellow
#URxvt.font:             xft:DejaVu Sans Mono:pixelsize=13:antialias=true
#URxvt.font:             xft:Inconsolata:pixelsize=18:antialias=true
URxvt.font:             xft:Terminus:pixelsize=14:antialias=false
#URxvt.boldFont:         xft:DejaVu Sans Mono:bold:pixelsize=13:antialias=true
#URxvt.boldFont:         xft:Inconsolata:bold:pixelsize=18:antialias=true
URxvt.boldFont:         xft:Terminus:pixelsize=14:antialias=false
URxvt.perl-ext-common:  default,tabbed
URxvt.title:            urxvt ftw!
URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      firefox-nightly
URxvt.matcher.button:   1
URxvt.scrollBar: False
URxvt.geometry: 140x40

